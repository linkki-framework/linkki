{
  "all-versions": [
    {
      "extension": "revapi.differences",
      "configuration": {
        "ignore": true,
        "differences": [
          {
            "regex": true,
            "code": "java.class.externalClassExposedInAPI",
            "new": ".*org\\.openqa\\.selenium\\..*",
            "justification": "Selenium is used in the API"
          },
          {
            "regex": true,
            "code": "java.class.externalClassExposedInAPI",
            "new": ".*org\\.junit\\.jupiter\\.api\\..*",
            "justification": "JUnit is used in the API"
          },
          {
            "regex": true,
            "code": "java.class.externalClassExposedInAPI",
            "new": ".*com\\.vaadin\\..*",
            "justification": "Vaadin is used in the API"
          },
          {
            "regex": true,
            "code": "java.class.externalClassExposedInAPI",
            "new": ".*org\\.linkki\\.core\\..*",
            "justification": "linkki is used in the API"
          }
        ]
      }
    }
  ],
  "2.1.0": [
    {
      "extension": "revapi.differences",
      "configuration": {
        "ignore": true,
        "differences": [
          {
            "regex": true,
            "code": "java.field.enumConstantOrderChanged",
            "classQualifiedName": "org.openqa.selenium.Platform"
          }
        ]
      }
    }
  ]
}
