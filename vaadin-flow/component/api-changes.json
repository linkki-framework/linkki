{
  "all-versions": [
    {
      "extension": "revapi.filter",
      "configuration": {
        "elements": {
          "exclude": [
            ".* com\\.vaadin\\.flow\\..*"
          ]
        }
      }
    },
    {
      "extension": "revapi.differences",
      "configuration": {
        "ignore": true,
        "differences": [
          {
            "regex": true,
            "code": "java.class.externalClassExposedInAPI",
            "package": "org\\.linkki\\.util.*",
            "justification": "linkki util API"
          }
        ]
      }
    }
  ]
}