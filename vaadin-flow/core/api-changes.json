{
    "all-versions": [
        {
            "extension": "revapi.filter",
            "configuration": {
                "elements": {
                    "exclude": [
                        ".* com\\.vaadin\\.flow\\..*"
                    ]
                }
            }
        },
        {
            "extension": "revapi.differences",
            "configuration": {
                "ignore": true,
                "differences": [
                    {
                        "regex": true,
                        "code": "java.class.externalClassExposedInAPI",
                        "new": ".*org\\.linkki\\.core\\..*",
                        "justification": "LIN-2651/LIN-2652"
                    },
                    {
                        "regex": true,
                        "code": "java.class.externalClassExposedInAPI",
                        "new": ".*org\\.linkki\\.util\\..*",
                        "justification": "LIN-2651/LIN-2652"
                    },
                    {
                        "regex": true,
                        "code": "java\\.field\\.added.*",
                        "new": "field .*",
                        "classQualifiedName": "org\\.linkki\\.core\\.util\\.LumoConstants.*",
                        "justification": "LumoConstants is just a copy of LumoUtility to avoid API Breaks."
                    },
                    {
                        "regex": true,
                        "code": "java\\.class\\.added.*",
                        "new": "class .*",
                        "classQualifiedName": "org\\.linkki\\.core\\.util\\.LumoConstants.*",
                        "justification": "LumoConstants is just a copy of LumoUtility to avoid API Breaks."
                    }
                ]
            }
        }
    ],
    "2.6.2": [
        {
            "extension": "revapi.differences",
            "configuration": {
                "ignore": true,
                "differences": [
                    {
