{
	"1.5.0": [
		{
			"extension": "revapi.differences",
			"configuration": {
				"ignore": true,
				"differences": [
					{
						"regex": true,
						"code": "java.missing.newClass",
						"new": "missing-class javax\\.servlet\\..*"
					},
					{
						"regex": true,
						"code": "java.missing.oldClass",
						"new": "missing-class javax\\.servlet\\..*"
					}
				]
			}
		}
	]
}