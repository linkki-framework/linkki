<FindBugsFilter>
    <!-- All bugs of rank greater 15 -->
    <Match>
        <Bug
            pattern="UPM_UNCALLED_PRIVATE_METHOD,
			BX_UNBOXING_IMMEDIATELY_REBOXED,
			DM_CONVERT_CASE,
			ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD,
			SIC_INNER_SHOULD_BE_STATIC_ANON,
			DLS_DEAD_LOCAL_STORE,
			BC_VACUOUS_INSTANCEOF,
			BC_UNCONFIRMED_CAST,
			RC_REF_COMPARISON_BAD_PRACTICE,
			SIC_INNER_SHOULD_BE_STATIC,
			UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR,
			NM_SAME_SIMPLE_NAME_AS_INTERFACE,
			CD_CIRCULAR_DEPENDENCY,
			SE_BAD_FIELD,
			DM_DEFAULT_ENCODING,
			USM_USELESS_SUBCLASS_METHOD,
			BIT_SIGNED_CHECK,
			EQ_UNUSUAL,
			IT_NO_SUCH_ELEMENT,
			EQ_DOESNT_OVERRIDE_EQUALS,
			EI_EXPOSE_REP2,
			BC_UNCONFIRMED_CAST_OF_RETURN_VALUE" />
    </Match>
    <Match>
        <!-- Because Find/SpotBugs currently is not fully compatible with Eclipse Null annotations -->
        <Bug code="NP" />
    </Match>

    <!-- All bugs in test classes, except for JUnit-specific bugs -->
    <Match>
        <Class name="~.*\.*Test" />
        <Not>
            <Bug code="IJU" />
        </Not>
    </Match>
</FindBugsFilter>
