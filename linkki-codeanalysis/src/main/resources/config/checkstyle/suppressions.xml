<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
    <suppress files=".*Messages.java" checks="[a-zA-Z0-9]*" />
    <!-- suppress all warnings in generated parser files -->
    <suppress files=".*Test.java" checks="[a-zA-Z0-9]*" />
    <suppress files=".*[\\/]target[\\/]generated-sources[\\/].*" checks="[a-zA-Z0-9]*" />
</suppressions>
