{
  "all-versions": [
    {
      "extension": "revapi.filter",
      "configuration": {
        "elements": {
          "exclude": [
            "missing-class com\\.google\\.gwt\\..*",
            ".*com.google.gwt:gwt-user:.*",
            ".*com.vaadin:vaadin-client:jar:.*",
            ".*com.vaadin:vaadin-shared:jar:.*",
            ".*org.linkki-framework:linkki-vaadin8-addon-shared:jar:.*"
          ]
        }
      }
    },
    {
      "extension": "revapi.differences",
      "configuration": {
        "ignore": true,
        "differences": [
          {
            "regex": true,
            "code": ".*",
            "newArchive": "com.google.gwt:gwt-user:jar:.*",
            "justification": "linkki Vaadin 14 changes"
          },
          {
            "regex": true,
            "code": ".*",
            "newArchive": "com.vaadin:vaadin-client:jar:.*",
            "justification": "linkki Vaadin 14 changes"
          },
          {
            "regex": true,
            "code": ".*",
            "newArchive": "com.vaadin:vaadin-shared:jar:.*",
            "justification": "linkki Vaadin 14 changes"
          },
          {
            "regex": true,
            "code": ".*",
            "newArchive": "org.linkki-framework:linkki-vaadin8-addon-shared:jar:.*",
            "justification": "linkki Vaadin 14 changes"
          },
          {
            "regex": true,
            "code": ".*",
            "classQualifiedName": "org\\.linkki\\.core\\.vaadin\\.component\\.client.*",
            "justifaction": "Indirect change"
          }
        ]
      }
    }
  ]
}
