{
  "all-versions": [
    {
      "extension": "revapi.filter",
      "configuration": {
        "elements": {
          "exclude": [
            "missing-class javax\\.servlet\\.http\\..*"
          ]
        }
      }
    }
  ],
  "1.4.2": [
    {
        "extension": "revapi.differences",
        "configuration": {
            "ignore": true,
            "justification": "LIN-2093/LIN-2180 :: Deprecate and refactor annotations",
            "differences": [
                {
                 "regex": true,
                  "code": ".*",
                  "oldArchive": "com.vaadin:vaadin-server:jar:8.13.1",
                  "newArchive": "com.vaadin:vaadin-server:jar:8.14.1",
                  "justification": "Updated vaadin version"
                },
                {
                  "regex": true,
                  "code": ".*",
                  "oldArchive": "org.jsoup:jsoup:jar:1.11.2",
                  "newArchive": "org.jsoup:jsoup:jar:1.14.2",
                  "justification": "Updated vaadin version"
                }
            ]
        }
    }
  ]
}
